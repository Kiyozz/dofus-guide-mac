generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./bdd.db"
}

model Player {
  id        Int    @id @default(autoincrement())
  name      String
  classe    Int
  gender    String
  guideId   Int
  lastEtape Int
}

model Cac {
  id       Int    @id @default(autoincrement())
  cacId    String @unique
  guideId  Int
  playerId Int
}
